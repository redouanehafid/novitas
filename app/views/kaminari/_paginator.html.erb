
<%= paginator.render do -%>
     
      <ul class="clearfix">
          <li><%= prev_page_tag unless current_page.first? %></li>
          <% each_page do |page| -%>
                <% if page.left_outer? || page.right_outer? || page.inside_window? -%>
                    <%= page_tag page %>
                <% elsif !page.was_truncated? -%>
                    <%= gap_tag %>
                <% end -%>
          <% end -%>
          <li><%= next_page_tag unless current_page.last? %></li>
       </ul>
      
<% end -%>
